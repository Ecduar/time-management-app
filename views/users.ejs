<!DOCTYPE html>
<html>
<head>
    <title>Usuarios</title>
</head>
<body>
    <h1>Lista de Usuarios</h1>
    <ul>
        <% users.forEach(user => { %>
            <li>
                <strong><%= user.name %></strong> - <%= user.email %>
                <a href="/users/<%= user.id %>">Ver detalles</a>
            </li>
        <% }) %>
    </ul>
    <a href="/">Volver al inicio</a>
</body>
</html>